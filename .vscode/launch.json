  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Distribute Version",
      "program": "${workspaceRoot}/libs/bin/distribute.js",
      "args": [
        "--version"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Distribute Help",
      "program": "${workspaceRoot}/libs/bin/distribute.js",
      "args": [
        "--help"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Distribute WD",
      "program": "${workspaceRoot}/libs/bin/distribute-wd.js",
      "args": []
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cordova",
      "program": "${workspaceRoot}/libs/bin/distribute-cordova.js",
      "args": [
        "1.8.5-patch6",
        "--config",
        "/Users/lcaprini/Development/mobile-app-distribution/distribute.json",
        "--android-version-code",
        "10805",
        "--ios-bundle-version",
        "185",
        "--tasks",
        "z",
        "-h",
        "-f"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Angular",
      "program": "${workspaceRoot}/libs/bin/distribute-angular.js",
      "args": [
        "2.0.0",
        "--config",
        "/Users/lcaprini/Development/mobile-app-distribution/distribute.json",
        "--tasks",
        "ue",
        "-f",
        "-v"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Serve",
      "program": "${workspaceRoot}/libs/bin/distribute-serve.js",
      "args": [
        "/Users/lcaprini/Development/mobile-app-distribution/app/www",
        "--port",
        "9002"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Distribute Resources",
      "program": "${workspaceRoot}/libs/bin/distribute-resources.js",
      "args": [
        "--icon",
        "${workspaceRoot}/examples/resources/icon.png",
        "--splash",
        "${workspaceRoot}/examples/resources/splash.png",
        "-p",
        "ia"
      ],
      "outputCapture": "std"
    },
    {
      "name": "Webpack",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/remote/wd/node_modules/.bin/webpack-dev-server",
      "args": [
        "--inline",
        "--hot"
      ],
      "outFiles": [
        "${workspaceRoot}/remote/wd/dist/*"
      ],
      "stopOnEntry": false,
      "cwd": "${workspaceRoot}/remote/wd/",
      "runtimeExecutable": null,
      "runtimeArgs": [],
      "env": {
        "NODE_ENV": "development"
      },
      "console": "internalConsole",
      "sourceMaps": true
    }
  ]
}